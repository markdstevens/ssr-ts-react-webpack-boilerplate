  "scripts": {
    "build:dev": "cross-env TS_NODE_PROJECT=\"tsconfig-webpack.json\" webpack --env.dev",
    "build": "cross-env TS_NODE_PROJECT=\"tsconfig-webpack.json\" webpack --env.prod",
    "prettier": "prettier --write src/**/*.{ts,tsx,js,css,json}",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix && stylelint 'src/**/*.scss' --fix",
    "tsm": "tsm src/**/*.module.scss",
    "test": "BABEL_ENV=test jest",
    "test:coverage": "yarn test -- --coverage",
    "watch:scss": "onchange src/**/*.module.scss -- yarn tsm",
    "start": "rimraf dist && yarn tsm && yarn codegen && yarn build && node dist/server.js",
    "start:dev": "rimraf dist && cat banner.txt && yarn tsm && yarn codegen && yarn build:dev",
    "generate": "ts-node --project bin/tsconfig.json bin/src/generator.ts",
    "codegen": "yarn codegen:stores && yarn codegen:controllers",
    "codegen:stores": "ts-node --project src/platform/codegen/tsconfig.json src/platform/codegen/store-boilerplate.ts",
    "codegen:controllers": "ts-node --project src/platform/codegen/tsconfig.json src/platform/codegen/controller-boilerplate.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn lint"
    }
  },

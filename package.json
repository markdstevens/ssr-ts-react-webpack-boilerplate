{
  "name": "react-ts",
  "version": "0.0.1",
  "main": "./dist/server.js",
  "author": "Mark Stevens <mdstevens94@gmail.com>",
  "license": "MIT",
  "scripts": {
    "webpack:profile:server": "pnpm run webpack -- --env.profileServer",
    "webpack:profile:client": "pnpm run webpack -- --env.profileClient",
    "webpack:dev": "webpack --env.dev",
    "webpack": "webpack --env.prod",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix && stylelint 'src/**/*.scss' --fix",
    "tsm": "tsm src/**/*.module.scss",
    "watch:scss": "onchange src/**/*.module.scss -- pnpm run tsm",
    "start": "rimraf dist && pnpm run tsm && pnpm run webpack && node dist/server.js",
    "start:dev": "rimraf dist && cat banner.txt && pnpm run tsm && pnpm run webpack:dev",
    "generate:route": "node scripts/create-route.js",
    "bundle-checks": "rimraf dist && pnpm run webpack:dev -- --env.nostart && node scripts/bundle-size-checker.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm run lint && pnpm run bundle-checks"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",

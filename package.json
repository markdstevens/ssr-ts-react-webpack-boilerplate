{
  "name": "react-ts",
  "version": "0.0.1",
  "main": "./dist/server.js",
  "author": "Mark Stevens <mdstevens94@gmail.com>",
  "license": "MIT",
  "scripts": {
    "webpack:dev": "tsc --lib es6 webpack.config.ts && webpack --env.dev && rm webpack.config.js",
    "webpack": "tsc --lib es6 webpack.config.ts && webpack --env.prod && rm webpack.config.js",
    "nodemon": "nodemon",
    "watch": "onchange 'src/**/*.{ts,tsx,scss}' -- pnpm run webpack:dev",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix && stylelint 'src/**/*.scss' --fix",
    "build": "rimraf dist && pnpm run webpack",
    "start": "pnpm run build && node dist/server.js",
    "build:dev": "rimraf dist && pnpm run webpack:dev",
    "start:dev": "pnpm run build:dev && npm-run-all -p watch nodemon"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@loadable/webpack-plugin": "^5.7.1",
    "@types/express": "^4.17.2",
    "@types/loadable__component": "^5.10.0",
    "@types/loadable__server": "^5.9.1",
    "@types/mini-css-extract-plugin": "^0.9.0",
    "@types/node": "^12.12.22",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/terser-webpack-plugin": "^2.2.0",
    "@types/webpack": "^4.41.1",
    "@types/webpack-bundle-analyzer": "^2.13.3",
    "@types/webpack-merge": "^4.1.5",
    "@types/webpack-node-externals": "^1.7.0",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "css-loader": "^3.4.0",
    "css-modules-typescript-loader": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "install": "^0.13.0",
